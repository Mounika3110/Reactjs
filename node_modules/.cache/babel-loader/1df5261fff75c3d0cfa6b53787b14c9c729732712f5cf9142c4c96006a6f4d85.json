\n  };\r\n  const removeFromCart = (id) => {\r\n    setCartItems(cartItems.filter(item => item.id !== id));\r\n  };\r\n  const updateQuantity = (id, newQuantity) => {\r\n    setCartItems(cartItems.map(item => \r\n      item.id === id ? { ...item, quantity: newQuantity } : item\r\n    ));\r\n  };\r\n  return (\r\n    <CartContext.Provider value={{ cartItems, addToCart, removeFromCart, updateQuantity }}>\r\n      {children}\r\n    </CartContext.Provider>\r\n  );\r\n};\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,aAAa,CAAEC,QAAQ,CAAEC,UAAU,KAAQ,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,yBACnE,KAAM,CAAAC,WAAW,cAAGL,aAAa,CAAC,CAAC,CACnC,MAAO,MAAM,CAAAM,OAAO,CAAGA,CAAA,GAAMJ,UAAU,CAACG,WAAW,CAAC,CACpD,MAAO,MAAM,CAAAE,YAAY,CAAGC,IAAA,EAAkB,IAAjB,CAAEC,QAAS,CAAC,CAAAD,IAAA,CACvC,KAAM,CAACE,SAAS,CAAEC,YAAY,CAAC,CAAGV,QAAQ,CAAC,EAAE,CAAC,CAC9C,KAAM,CAAAW,SAAS,CAAIC,OAAO,EAAK,CAC7B,KAAM,CAAAC,eAAe,CAAGJ,SAAS,CAACK,IAAI,CAACC,IAAI,EAAIA,IAAI,CAACC,EAAE,GAAKJ,OAAO,CAACI,EAAE,CAAC,CACtE,GAAIH,eAAe,CAAE,CACnBH,YAAY,CAACD,SAAS,CAACQ,GAAG,CAACF,IAAI,EAC7BA,IAAI,CAACC,EAAE,GAAKJ,OAAO,CAACI,EAAE,CAAG,CAAE,GAAGD,IAAI,CAAEG,QAAQ,CAAEH,IAAI,CAACG,QAAQ,CAAG,CAAE,CAAC,CAAGH,IACtE,CAAC,CAAC,CACJ,CAAC,IAAM,CACLL,YAAY,CAAC,CAAC,GAAGD,SAAS,CAAE,CAAE,GAAGG,OAAO,CAAEM,QAAQ,CAAE,CAAE,CAAC,CAAC,CAAC,CAC3D,CACF,CAAC,CACD,KAAM,CAAAC,cAAc,CAAIH,EAAE,EAAK,CAC7BN,YAAY,CAACD,SAAS,CAACW,MAAM,CAACL,IAAI,EAAIA,IAAI,CAACC,EAAE,GAAKA,EAAE,CAAC,CAAC,CACxD,CAAC,CACD,KAAM,CAAAK,cAAc,CAAGA,CAACL,EAAE,CAAEM,WAAW,GAAK,CAC1CZ,YAAY,CAACD,SAAS,CAACQ,GAAG,CAACF,IAAI,EAC7BA,IAAI,CAACC,EAAE,GAAKA,EAAE,CAAG,CAAE,GAAGD,IAAI,CAAEG,QAAQ,CAAEI,WAAY,CAAC,CAAGP,IACxD,CAAC,CAAC,CACJ,CAAC,CACD,mBACEZ,IAAA,CAACC,WAAW,CAACmB,QAAQ,EAACC,KAAK,CAAE,CAAEf,SAAS,CAAEE,SAAS,CAAEQ,cAAc,CAAEE,cAAe,CAAE,CAAAb,QAAA,CACnFA,QAAQ,CACW,CAAC,CAE3B,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}